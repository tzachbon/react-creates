{
	"name": "react-creates-vsc",
	"displayName": "React Creates",
	"description": "React creates for you useful and common tools that adapt themselves to your project for faster and easier development",
	"version": "0.0.23",
	"publisher": "TzachBonfil",
	"engines": {
		"vscode": "^1.45.0"
	},
	"keywords": [
		"react",
		"component",
		"cli",
		"generator",
		"tool",
		"react-native",
		"react native",
		"native",
		"ng add",
		"javascript",
		"typescript"
	],
	"categories": [
		"Other"
	],
	"icon": "assets/logo.png",
	"homepage": "https://github.com/tzachbon/react-creates/tree/master/packages/react-creates-vsc/README.md",
	"repository": {
		"url": "https://github.com/tzachbon/react-creates/tree/master/packages/react-creates-vsc.git",
		"type": "github"
	},
	"activationEvents": [
		"*",
		"onCommand:react-creates-vsc.component",
		"onCommand:react-creates-vsc.create"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-creates-vsc.component",
				"title": "React Creates: Component",
				"icon": {
					"dark": "assets/add_icon_light.svg",
					"light": "assets/add_icon_dark.svg"
				}
			},
			{
				"command": "react-creates-vsc.create",
				"title": "React Creates: Create"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"when": "explorerResourceIsFolder",
					"command": "react-creates-vsc.component"
				}
			],
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "react-creates-vsc.component",
					"group": "reactcreates@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"clean": "rm -rf out",
		"compile": "tsc ",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch ./",
		"pretest": "npm run clean && npm run compile && npm run lint",
		"test": "cp ./package.json ./src/test/assets && node ./out/test/runTest.js",
		"deploy": "node deploy"
	},
	"devDependencies": {
		"@types/chai": "^4.2.12",
		"@types/execa": "^2.0.0",
		"@types/find-package-json": "^1.1.1",
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.159",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/promise-timeout": "^1.3.0",
		"@types/sinon": "^9.0.4",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"chai": "^4.2.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"sinon": "^9.0.3",
		"ts-loader": "^8.0.2",
		"ts-mocha": "^7.0.0",
		"utility-types": "^3.10.0",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"configstore": "^5.0.1",
		"execa": "^4.0.3",
		"find-package-json": "^1.2.0",
		"lodash": "^4.17.19",
		"react-creates": "^1.0.23"
	}
}
