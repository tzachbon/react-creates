{
	"name": "react-creates-vsc",
	"displayName": "react-creates",
	"description": "React creates for you",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:react-creates-vsc.create"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-creates-vsc.create",
				"title": "React Creates"
			}
		],
		"menus": {
			"commandPalette": []
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/execa": "^2.0.0",
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.159",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"execa": "^4.0.3",
		"lodash": "^4.17.19",
		"react-creates": "latest"
	}
}
