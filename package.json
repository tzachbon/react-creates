{
  "name": "react-creates",
  "version": "1.0.0",
  "description": "Create React Component folder works on macOS, Windows, and Linux.",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "react-creates": "dist/react-creates.js"
  },
  "scripts": {
    "clean": "rm -rf __temp__",
    "build": "tsc",
    "predev": "rm -rf dist && tsc && npm link",
    "dev": "tsc -w",
    "lint": "eslint --fix --ext .js,.ts .",
    "test": "jest --no-cache --runInBand 2>&1",
    "create:cmp": "npm run clean && react-creates component __temp__  -f --scss -d __temp__ "
  },
  "keywords": [
    "react",
    "component",
    "cli",
    "generator",
    "tool",
    "react-native",
    "react native",
    "native",
    "ng add"
  ],
  "author": {
    "name": "Tzach Bonfil",
    "email": "tzachbonfil@gmail.com",
    "url": "https://github.com/tzachbon"
  },
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "inquirer": "^7.3.2",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "mustache": "^4.0.1",
    "ncp": "^2.0.0",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "26.0.0",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.157",
    "@types/mustache": "^4.0.1",
    "@types/ncp": "^2.0.4",
    "@types/node": "14.0.13",
    "@types/tempy": "^0.3.0",
    "create-react-app": "^3.4.1",
    "execa": "^4.0.3",
    "jest": "^26.1.0",
    "tempy": "^0.5.0",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  }
}
